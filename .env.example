# VibeCoding Quantum Trading Platform Environment Variables
# Copy this file to .env and fill in your actual values

# Wallet Configuration
WALLET_PRIVATE_KEY=your_base64_private_key_here
WALLET_PUBLIC_KEY=your_solana_wallet_address_here

# Database Configuration
DATABASE_URL=postgresql://user:password@host:port/database

# API Keys (if needed)
# OPENAI_API_KEY=your_openai_key_here
# ANTHROPIC_API_KEY=your_anthropic_key_here

# Application Configuration
NODE_ENV=development
PORT=3000

# Security Configuration
QUANTUM_SECURITY_LEVEL=maximum
CONSCIOUSNESS_VERIFICATION=enabled